;\r\n            }}\r\n          >\r\n             {categories &&\r\n              categories.map((category) => (\r\n                <option key={category.docId} value={category.category}>{category.category}</option>          \r\n              ))}\r\n        {/*     <option value=\"Familj\">Familj</option>\r\n            <option value=\"Rån\">Rån</option>\r\n            <option value=\"Misshandel\">Misshandel</option> */}\r\n          </select>\r\n          <label>Jag vill vara Anonym</label>\r\n          <input type=\"checkbox\" onClick={toggleCheckbox} />\r\n          <Button disabled={loading} type=\"submit\">\r\n            Publicera\r\n          </Button>\r\n        </Form>\r\n        <LinkDiv>\r\n          <CancelLink to=\"/profile\">Cancel</CancelLink>\r\n        </LinkDiv>\r\n      </Container>\r\n    </>\r\n  );\r\n}\r\n","C:\\Users\\angie\\OneDrive\\Skrivbord\\EXjobb\\Auth\\authentication-app\\src\\Page\\UpdatePostPage.jsx",[],"C:\\Users\\angie\\OneDrive\\Skrivbord\\EXjobb\\Auth\\authentication-app\\src\\components\\UpdatePost.jsx",["249","250","251"],"C:\\Users\\angie\\OneDrive\\Skrivbord\\EXjobb\\Auth\\authentication-app\\src\\components\\SelectCategories.jsx",["252","253"],"C:\\Users\\angie\\OneDrive\\Skrivbord\\EXjobb\\Auth\\authentication-app\\src\\components\\GetCategories.jsx",["254"],"C:\\Users\\angie\\OneDrive\\Skrivbord\\EXjobb\\Auth\\authentication-app\\src\\components\\styles\\FormStyle.js",[],"C:\\Users\\angie\\OneDrive\\Skrivbord\\EXjobb\\Auth\\authentication-app\\src\\components\\styles\\DetailPageStyle.js",[],"C:\\Users\\angie\\OneDrive\\Skrivbord\\EXjobb\\Auth\\authentication-app\\src\\components\\styles\\GeneralStyle.js",["255"],"import { NavLink as Link } from 'react-router-dom';\r\nimport styled from \"styled-components\";\r\n\r\nexport const Container = styled.div`\r\n background: aliceblue;\r\n display: flex;\r\n flex-direction: column;\r\n align-items: center;\r\n//  background: rgb(150 132 132);\r\n\r\n`;\r\nexport const SocialBtnCo